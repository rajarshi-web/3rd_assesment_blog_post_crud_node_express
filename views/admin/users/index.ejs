<h3>Users</h3>

<table class="table table-bordered bg-white">
<tr>
<th>Name</th>
<th>Email</th>
<th>Action</th>
</tr>

<% if(users && users.length){ %>
<% users.forEach(u => { %>
<tr>
<td><%= u.name %></td>
<td><%= u.email %></td>
<td>
<a href="/admin/users/delete/<%= u._id %>"
class="btn btn-danger btn-sm">Delete</a>
</td>
</tr>
<% }) %>
<% } else { %>
<tr><td colspan="3">No users found</td></tr>
<% } %>
</table>
